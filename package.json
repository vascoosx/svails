{
  "name": "svails",
  "version": "1.0.0",
  "description": "svelte on rails",
  "directories": {
    "bin": "vendor/svails"
  },
  "scripts": {
    "build": "cd svelte_js && npx rollup -c",
    "postinstall": "cd svelte_js && npm ci",
    "manifest": "node vendor/svails/make_manifest.js",
    "watch": "onchange 'svelte_js/public/build' -- npm run manifest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vascoosx/svails.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vascoosx/svails/issues"
  },
  "homepage": "https://github.com/vascoosx/svails#readme",
  "devDependencies": {
    "md5": "^2.2.1",
    "md5-file": "^5.0.0",
    "onchange": "^7.0.2"
  }
}
